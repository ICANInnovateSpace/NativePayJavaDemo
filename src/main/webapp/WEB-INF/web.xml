<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>nativePayServlet</servlet-name>
    <servlet-class>com.ican.servlet.NativePayServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>orderQueryServlet</servlet-name>
    <servlet-class>com.ican.servlet.OrderQueryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>orderQueryServlet</servlet-name>
    <url-pattern>/OrderQueryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>nativePayServlet</servlet-name>
    <url-pattern>/NativePayServlet</url-pattern>
  </servlet-mapping>
</web-app>
